{
  "name": "webquery-dev",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.1.0",
  "scripts": {
    "dev": "pnpm packages:dev",
    "build": "pnpm packages:build",
    "packages:dev": "pnpm -r --filter '@webquery-dev/*' --parallel run dev",
    "packages:build": "pnpm -r --filter '@webquery-dev/*' run build",
    "packages:stub": "pnpm -r --filter '@webquery-dev/*' run stub",
    "packages:publish": "pnpm -r --filter '@webquery-dev/*' run package:publish",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "pnpm -r --filter '@webquery-dev/*' --parallel run typecheck"
  },
  "dependencies": {},
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.0",
    "@unocss/eslint-config": "^65.4.3",
    "eslint": "^9.19.0",
    "eslint-plugin-format": "^1.0.1",
    "typescript": "^5.7.3"
  }
}
